{
    "name": "web-id-fr/laravel-report-exceptions",
    "description": "A simple Laravel 5+ package to report non-catched exceptions by mail.",
    "keywords": [
        "laravel",
        "email",
        "log",
        "report",
        "email-log",
        "exception",
        "error"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "^6.18.43"
    },
    "autoload": {
        "psr-4": {
            "WebID\\LaravelReportExceptions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebID\\LaravelReportExceptions\\LaravelReportExceptionsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Wilfried JOURDIL",
            "email": "wilfried@web-id.fr",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
